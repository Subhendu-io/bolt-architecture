    "crypto-js": "^4.0.0",
    "express": "^4.17.1",
    "express-enforces-ssl": "^1.1.0",
    "express-validator": "^6.6.0",
    "feature-policy": "^0.5.0",
    "forever": "^3.0.0",
    "googleapis": "^55.0.0",
    "helmet": "^3.23.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "module-alias": "^2.2.2",
    "moment": "^2.27.0",
    "mongoose": "^5.9.25",
    "os": "^0.1.1",
    "path": "^0.12.7",
    "query-string": "^6.13.1",
