  "scripts": {
    "start": "forever backend/app.js",
    "dev": "nodemon backend/app.js",
    "test": "nodemon backend/app.js",
    "build": "npm run build-dev",
    "build-dev": "cd frontend && ng build --configuration=dev",
    "build-beta": "cd frontend && ng build --configuration=beta",
    "build-prod": "cd frontend && && ng build --prod",
    "heroku-prebuild": "cd frontend && npm install -g @angular/cli && npm install && npm install --only=dev && cd .."
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
