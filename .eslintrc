{
  "rules": {
      "semi": ["error", "always"],
      "quotes": ["error", "single"],
      "indent": ["error", 2],
      "no-trailing-spaces": 1,
      "key-spacing": ["error", {"multiLine": {"beforeColon": false, "afterColon": true },"align": {"beforeColon": true,"afterColon": true,"on": "colon" },"singleLine":{"beforeColon": false, "afterColon": true}}],
      "comma-spacing": [1, {"before": false, "after": true}],
      "brace-style": ["error", "1tbs", {"allowSingleLine": true}],
      "no-multi-spaces": "error",
      "space-infix-ops": "error",
      "no-tabs": ["error", { "allowIndentationTabs": true }],
      "no-multiple-empty-lines": ["error", { "max": 1, "maxEOF": 1 }],
      "space-before-blocks": "error",
      "camelcase": ["error", {"ignoreDestructuring": true}],
      "block-scoped-var": "error",
      "keyword-spacing": ["error", { "before": true, "after": true ,"overrides": {"if": { "after": false },"for": { "after": false },"while": { "after": false }}}],
      "capitalized-comments": ["error", "never"],
      "no-dupe-keys": "error",
      "default-case": "error",
      "dot-location": ["error", "property"],
      "no-redeclare": "error",
      "computed-property-spacing": ["error", "never"],
      "operator-linebreak": ["error", "after", { "overrides": { "?": "ignore", ":": "ignore" } }],
      "no-undef": ["error", { "typeof": true }],
      "getter-return": ["error", { "allowImplicit": true }],
      "for-direction": "error",
      "no-cond-assign": ["error", "always"],
      "no-duplicate-case": "error",
      "no-empty": "error",
      "no-ex-assign": "error",
      "no-empty-character-class": "error",
      "no-extra-boolean-cast": "error",
      "no-extra-parens": ["error", "all", { "returnAssign": false, "conditionalAssign": false }],
      "no-extra-semi": "error",
      "no-func-assign": "error",
      "no-obj-calls": "error",
      "no-sparse-arrays": "error",
      "no-unreachable": "error",
      "no-unsafe-finally": "error",
      "valid-typeof": "error",
      "no-unused-vars": ["error", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }]
  },
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
    "es6": true,
    "node": true
  }
}